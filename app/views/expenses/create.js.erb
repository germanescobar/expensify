<% if @expense.errors.empty? %>
  $('#js-new-expense-modal').modal('hide');
  $('#expenses-table tbody').prepend('<%= j render @expense %>');
  $('#js-alert-push').html('<%= j render "shared/alert", type: "success", message: expense_successful_message(@expense, "created") %>');
<% else %>
  $('#js-form-errors-box').html('<%= j render "shared/error_messages", object: @expense %>')
<% end %>